import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int count = sc.nextInt();
		int[] arr = new int[count];
		int h = 0;
		for (int i = 0; i < count; i++) {
			arr[i] = sc.nextInt();
		}
		for (int i = 0; i < count/2; i++) {
			if (arr[i] == arr[i + (count/2)]) {
				h+=2;
			}
		}
		System.out.println(h);
	}
}
