#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {

    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    vector<vector<int>>x;

    for (int i = 0; i < n; i++) {
        vector<int> y(n);
        for (int j = 0; j < n; j++) {
            cin >> y[j];
        }
        x.push_back(y);
    }

    int c1, c2, c3, c4;
    c1 = x[0][0];
    c2 = x[0][n-1];
    c3 = x[n-1][0];
    c4 = x[n-1][n-1];

    if (min(c1, min(c2, min(c3, c4))) == c1) {
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                cout << x[i][j] << " ";
            }
            cout << "\n";
        }
    } else if (min(c1, min(c2, min(c3, c4))) == c2) {
        for (int i = n-1; i >= 0; i--) {
            for (int j = 0; j < n; j++) {
                cout << x[j][i] << " ";
            }
            cout << "\n";
        }
    } else if ((min(c1, min(c2, min(c3, c4))) == c3)) {
        for (int i = 0; i < n; i++) {
            for (int j = n-1; j >= 0; j--) {
                cout << x[j][i] << " ";
            } 
            cout << "\n";
        }
    } else {
        for (int i = n-1; i >= 0; i--) {
            for (int j = n-1; j >= 0; j--) {
                cout << x[i][j] << " ";
            } 
            cout << "\n";
        }
    }

    return 0;
}
